{
  "name": "react-starter",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "browserify-css": {
    "autoInject": true,
    "minify": true,
    "rootDir": "."
  },
  "scripts": {
    "build": "NODE_ENV=production browserify -t browserify-css src/app.js | uglifyjs -cm -t browserify-css > public/bundle.js",
    "start": "ecstatic -p 8001 public",
    "watch": "watchify -t browserify-css src/app.js -o public/bundle.js -dv"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^10.2.6",
    "cldr": "^3.5.2",
    "cldr-data": "^30.0.1",
    "ecstatic": "~0.8.0",
    "globalize": "^1.1.2",
    "globalizer": "^0.2.2",
    "jquery": "^3.1.0",
    "jquery-ajax-transport-xdomainrequest": "^1.0.4",
    "moment": "^2.17.0",
    "react": "^0.14.8",
    "react-big-calendar": "^0.11.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^0.14.8",
    "react-globalize": "^0.3.10",
    "react-router": "^3.0.0",
    "reactify": "^1.1.1",
    "string-format": "^0.5.0",
    "uglify-js": "^2.4.24",
    "watchify": "^3.2.3"
  },
  "license": "public domain",
  "devDependencies": {
    "browserify-css": "^0.9.2"
  }
}
